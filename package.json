{
  "name": "semantic-code-indexer",
  "version": "1.0.0",
  "description": "A semantic code indexer that parses JavaScript/TypeScript files using Tree-sitter to extract semantic chunks",
  "main": "dist/index.js",
  "bin": {
    "semantic-indexer": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsc && node dist/cli.js"
  },
  "keywords": [
    "tree-sitter",
    "ast",
    "semantic",
    "indexer",
    "code-analysis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^14.0.2",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.21.2",
    "tree-sitter-typescript": "^0.23.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
