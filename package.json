{
  "name": "semantic-code-indexer",
  "version": "1.0.0",
  "description": "A semantic code indexer that parses JavaScript/TypeScript files using ts-morph to extract semantic chunks",
  "main": "dist/index.js",
  "bin": {
    "semantic-indexer": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsc && node dist/cli.js"
  },
  "keywords": [
    "ts-morph",
    "ast",
    "semantic",
    "indexer",
    "code-analysis",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^14.0.2",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  }
}
